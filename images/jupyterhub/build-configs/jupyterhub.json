{
    "kind": "List",
    "apiVersion": "v1",
    "items": [
        {
            "kind": "ImageStream",
            "apiVersion": "image.openshift.io/v1",
            "metadata": {
                "name": "jupyterhub",
                "labels": {
                    "build": "jupyterhub"
                }
            },
            "spec": {
                "lookupPolicy": {
                    "local": true
                }
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "build.openshift.io/v1",
            "metadata": {
                "name": "jupyterhub",
                "labels": {
                    "build": "jupyterhub"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "type": "ImageChange"
                    }
                ],
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/CODA-19/coda19-jupyterhub.git",
                        "ref": "feature/deploi-images"
                    },
                    "contextDir": "images/jupyterhub"                    
                },
                "strategy": {
                    "type": "Source",
                    "sourceStrategy": {
                        "from": {
                            "kind": "DockerImage",
                            "name": "registry.hub.docker.com/centos/python-36-centos7:latest"
                        }
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "jupyterhub:3.4.0"
                    }
                }
            }
        }
    ]
}
